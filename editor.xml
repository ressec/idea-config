<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
    <option name="COLLAPSE_FILE_HEADER" value="false" />
  </component>
  <component name="CodeInsightSettings">
    <option name="SHOW_PARAMETER_NAME_HINTS_ON_COMPLETION" value="true" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="JAVADOC_INFO_DELAY" value="500" />
    <option name="SHOW_FULL_SIGNATURES_IN_PARAMETER_INFO" value="true" />
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="CSS" value="true" />
        <entry key="FTL" value="true" />
        <entry key="Groovy" value="true" />
        <entry key="HTML" value="true" />
        <entry key="Haml" value="true" />
        <entry key="JAVA" value="true" />
        <entry key="JSON" value="true" />
        <entry key="JSP" value="true" />
        <entry key="JSPX" value="true" />
        <entry key="JavaScript" value="true" />
        <entry key="LESS" value="true" />
        <entry key="Markdown" value="true" />
        <entry key="SASS" value="true" />
        <entry key="SCSS" value="true" />
        <entry key="SQL" value="true" />
        <entry key="Stylus" value="true" />
        <entry key="TypeScript" value="true" />
        <entry key="VTL" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="kotlin" value="false" />
        <entry key="protobuf" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseConstructorGenericParameters" value="false" />
    <option name="collapseLambdas" value="false" />
    <option name="collapseOneLineMethods" value="false" />
    <option name="collapseSuppressWarnings" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bcaution\b.*">
      <option name="FOREGROUND" value="c04964" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\breminder\b.*">
      <option name="FOREGROUND" value="c06d09" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\banju\b.*">
      <option name="FOREGROUND" value="c04ab0" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bperf\b.*">
      <option name="FOREGROUND" value="c03630" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\bquestion\b.*">
      <option name="FOREGROUND" value="5fb1c0" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <filter name="CAUTION">
      <pattern index="2" />
    </filter>
    <filter name="TODO">
      <pattern index="0" />
    </filter>
    <filter name="FIXME">
      <pattern index="1" />
    </filter>
    <filter name="PERFORMANCE">
      <pattern index="5" />
    </filter>
    <filter name="ANJU">
      <pattern index="4" />
    </filter>
    <filter name="REMINDER">
      <pattern index="3" />
    </filter>
    <filter name="QUESTION">
      <pattern index="6" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>